#include <algorithm> 
#include <iostream> 

using namespace std;


template <typename T>
T findLargest(T a, T b, T c) {
    return max(a, max(b, c));
}

int main() {
    
    double a, b, c;

    
    cout << "Enter three numbers:" << endl;
    
    cout << "Number 1: ";
    cin >> a;

    cout << "Number 2: ";
    cin >> b;

    cout << "Number 3: ";
    cin >> c;

   
    double largest = findLargest(a, b, c);
    cout << "The largest number is: " << largest << endl;

    return 0;
}
